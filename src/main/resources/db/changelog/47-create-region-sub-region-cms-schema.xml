<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="create_region_sub_region" author="sheng.w">
        <createTable tableName="region_sub_region">

            <column name="region_id" type="varchar(255)">
                <constraints nullable="false" />
            </column>

            <column name="sub_region_id" type="varchar(255)">
                <constraints nullable="false" />
            </column>

        </createTable>
    </changeSet>

    <changeSet id="tag-cms-1.47" author="indira">
        <tagDatabase tag="cms-1.47" />
    </changeSet>
</databaseChangeLog>